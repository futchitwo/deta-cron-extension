{
  "name": "deta-random-cron",
  "version": "0.2.0-alpha-5",
  "description": "Set random cron to deta micro",
  "main": "lib.js",
  "types": "lib.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "vitest run --globals --dir=test --isolate=true",
    "build": "esbuild ./lib/ --platform=node --bundle --external:deta --external:aws-cron-parser --external:node-fetch --outfile=lib.js --target=node14",
    "buildmin": "esbuild ./lib/ --minify --bundle --platform=node --external:aws-cron-parser --external:deta --external:node-fetch --outfile=lib.min.js --target=node14"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-cron-parser": "^1.1.12",
    "deta": "^1.1.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "better-typescript-lib": "^2.1.0",
    "esbuild": "^0.15.2",
    "typescript": "^4.7.4",
    "vitest": "^0.21.1"
  },
  "files": [
    "lib.js",
    "lib.d.ts"
  ]
}
